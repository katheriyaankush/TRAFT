{
    "name": "traft",
    "version": "1.0.0",
    "description": "TRAFT framework",
    "main": "index.js",
    "scripts": {
        "test": "npx cucumber-js -p runner & npx ts-node ./src/support/reporter/HTMLReporter.ts & npx ts-node ./src/support/reporter/CucumberReporter.ts",
        "failed:test": "npx cucumber-js -p rerun",
        "report": "npx ts-node ./src/support/reporter/HTMLReporter.ts & npx ts-node ./src/support/reporter/CucumberReporter.ts",
        "dry:test": "npx cucumber-js -p runner --dry-run",
        "gherkin-lint": "gherkin-lint -c .gherkin-lintrc",
        "eslint": "eslint . --ext .js,.ts",
        "eslint-fix": "eslint ./ --ext .js,.ts --fix",
        "prepare": "husky install",
        "format": "prettier --write \"**/*.{ts,js}\" ",
        "pretest": "npm run prepare",
        "ng":"ng",
        "build":"ng build",
        "buildTest":"ng test"
    }, 
    "keywords": [],
    "author": "Tredence-Studio",
    "license": "ISC",
    "devDependencies": {
        "@cucumber/cucumber": "8.7.0",
        "@cucumber/pretty-formatter": "1.0.0",
        "@playwright/test": "1.27.1",
        "@types/cucumber-html-reporter": "5.0.1",
        "@types/string-format": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "chance": "^1.1.11",
        "cross-env": "7.0.3",
        "dotenv": "16.0.0",
        "eslint": "^8.16.0",
        "eslint-config-airbnb-typescript": "16.1.4",
        "eslint-config-prettier": "8.10.0",
        "eslint-plugin-playwright": "0.8.0",
        "eslint-plugin-prettier": "5.0.0",
        "fs-extra": "10.1.0",
        "gherkin-lint": "^4.2.2",
        "husky": "^8.0.3",
        "multiple-cucumber-html-reporter": "^1.21.6",
        "playwright": "1.27.1",
        "prettier": "3.0.1",
        "ts-node": "10.9.1",
        "typescript": "4.8.4",
        "winston": "3.8.2"
    },
      "dependencies": {
        "crypto-js": "^4.1.1",
        "cucumber-html-reporter": "^7.1.1",
        "fetch-to-curl": "^0.6.0",
        "jsonpath": "^1.1.1",
        "luxon": "^3.3.0"
    }
}
